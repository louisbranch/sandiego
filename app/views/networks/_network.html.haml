%div#network_witness
  %div
    = image_tag(network.location.witness_photo_path)
    %div#witness_name= network.location.witness
  %div#witness_info= network.informable.description
- if time_traveled
  %div#location_time_traveled{'data-growl' => image_path("clock.png")}= "#{time_traveled} horas de viagem"
- if traits_found
  %div#traits_found{'data-growl' => image_path("magnifying-glass.png")}= traits_found
